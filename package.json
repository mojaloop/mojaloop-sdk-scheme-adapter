{
  "name": "@mojaloop/sdk-scheme-adapter",
  "version": "15.0.0",
  "description": "mojaloop sdk-scheme-adapter",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/sdk-scheme-adapter",
  "private": true,
  "bugs": {
    "url": "https://github.com/mojaloop/sdk-scheme-adapter/issues#readme"
  },
  "contributors": [
    "Miguel de Barros <miguel.debarros@modusbox.com>",
    "Shashikant Hirugade <shashikant.hirugade@modusbox.com>",
    "Vijay Kumar Guthi <vijaya.guthi@modusbox.com>"
  ],
  "packageManager": "yarn@3.2.0",
  "engines": {
    "yarn": "3.2.0"
  },
  "workspaces": [
    "modules/private-shared-lib",
    "modules/public-shared-lib",
    "modules/*"
  ],
  "scripts": {
    "start": "yarn workspaces foreach --parallel -i -v run start",
    "start1": "nx affected --parallel --target=start",
    "start:api-svc": "yarn workspace @mojaloop/sdk-scheme-adapter-api-svc run start",
    "start:event-handler": "yarn workspace @mojaloop/sdk-scheme-adapter-event-handler run start",
    "start:command-handler": "yarn workspace @mojaloop/sdk-scheme-adapter-command-handler run start",
    "build": "nx run-many --all --target=build",
    "build:all": "nx affected --target=build",
    "build:openapi": "yarn workspace @mojaloop/sdk-scheme-adapter-api-svc run build:openapi",
    "build:openapi:inbound": "yarn workspace @mojaloop/sdk-scheme-adapter-api-svc run build:openapi:inbound",
    "build:openapi:outbound": "yarn workspace @mojaloop/sdk-scheme-adapter-api-svc run build:openapi:outbound",
    "validate:api": "yarn workspace @mojaloop/sdk-scheme-adapter-api-svc run validate:api",
    "watch": "nx run-many --parallel --all --target=watch",
    "clean": "yarn run clean:dist && yarn run clean:npm",
    "clean:npm": "nx run-many --parallel --all --target=clean:npm",
    "clean:dist": "nx run-many --parallel --all --target=clean:dist",
    "pretest": "echo 'FIX ME'",
    "lint": "nx affected --parallel --all --target=lint",
    "lint:fix": "yarn run lint -- --fix",
    "test:unit": "nx affected --parallel --all --target=test:unit",
    "test:unit:all": "nx run-many --parallel --all --target=test:unit",
    "test:coverage": "nx affected --parallel --all --target=test:coverage",
    "test:coverage:all": "nx run-many --parallel --all --target=test:coverage",
    "test:coverage-check": "nx affected --parallel --all --target=test:coverage-check",
    "test:coverage-check:all": "nx run-many --parallel --all --target=test:coverage-check",
    "test:integration": "yarn workspaces foreach --parallel -v run test:integration",
    "audit:check": "yarn dlx audit-ci --config ./audit-ci.jsonc && yarn workspaces foreach --parallel -v run audit:check",
    "dep:check": "nx run-many --parallel --all --target=dep:check",
    "dep:update": "nx run-many --parallel --all --target=dep:update",
    "docker:build": "nx affected --parallel --all --target=docker:build",
    "docker:publish": "nx affected --parallel --all --target=docker:publish",
    "npm:publish": "nx affected --parallel --all --target=npm:publish"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@types/node-cache": "^4.2.5",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "audit-ci": "^6.3.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "latest",
    "husky": "^8.0.1",
    "jest": "^27.3.1",
    "npm-check-updates": "^12.5.11",
    "nx": "14.5.7",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "yarn-audit-fix": "^9.3.2"
  },
  "dependencies": {
    "nodemon": "^2.0.18",
    "tslib": "^2.4.0"
  }
}
